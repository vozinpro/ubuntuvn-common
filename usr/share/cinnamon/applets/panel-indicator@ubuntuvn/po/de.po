#: applet.js:51
msgid "Panel Edit mode"
msgstr "Leistenbearbeitungsmodus"

#: applet.js:57
msgid "Add applets to the panel"
msgstr "Applets zur Leiste hinzufügen"

#. panel-indicator@ubuntuvn->metadata.json->description
msgid ""
"hide a few applet to have a neater panel, and make them show up again by a "
"click"
msgstr ""
"Verstecke ein paar Applets, damit die Leiste aufgeräumter aussieht. Ein "
"einfacher Klick zeigt sie wieder."

#. panel-indicator@ubuntuvn->metadata.json->name
msgid "Drawer (show/hide applets)"
msgstr "Schublade (zeige/verstecke Applets)"

#. panel-indicator@ubuntuvn->settings-
#. schema.json->hoveractivateshide->description
msgid "Hide on hovering again"
msgstr "Verstecke beim Darüberbewegen wieder"

#. panel-indicator@ubuntuvn->settings-
#. schema.json->hoveractivateshide->tooltip
msgid "toggle visibility of applets on mouse hover"
msgstr "Schalte die Sichtbarkeit der Applets beim Darüberbewegen der Maus um"

#. panel-indicator@ubuntuvn->settings-
#. schema.json->autohiderstime->description
msgid "Reshowers' autohide delay: "
msgstr "Verstecke wiederauftauchender Applets nach"

#. panel-indicator@ubuntuvn->settings-schema.json->autohiderstime->units
#. panel-indicator@ubuntuvn->settings-schema.json->hidetime->units
msgid "seconds"
msgstr "Sekunden"

#. panel-indicator@ubuntuvn->settings-
#. schema.json->disablestarttimeautohide->description
msgid "Disable start time autohide"
msgstr "Deaktiviere Verzögerung für das automatische Verstecken"

#. panel-indicator@ubuntuvn->settings-
#. schema.json->disablestarttimeautohide->tooltip
msgid "if not in autohide mode"
msgstr "Wenn \"Verstecke automatisch\" nicht aktiviert ist."

# Other applet do not show the internal applet name. We should therefore just call this "Settings" and nothing more.
#. panel-indicator@ubuntuvn->settings-schema.json->head->description
msgid "Settings for panel-indicator@ubuntuvn"
msgstr "Einstellungen"

#. panel-indicator@ubuntuvn->settings-schema.json->autohide->description
msgid "Autohide"
msgstr "Verstecke automatisch"

#. panel-indicator@ubuntuvn->settings-schema.json->autohide->tooltip
msgid ""
"check if you want to hide applets automatically, unless you should click on "
"applet to do so"
msgstr ""
"Aktiviere diese Funktion, wenn du Applets automatisch verstecken möchtest, "
"ansonsten werden sie nur bei einem Mausklick versteckt."

#. panel-indicator@ubuntuvn->settings-
#. schema.json->autohiders->description
msgid "Autohide reshowing applets"
msgstr "Verstecke wiederauftauchende Applets automatisch"

#. panel-indicator@ubuntuvn->settings-schema.json->hovertime->description
msgid "Hover activates/hides after "
msgstr "Aktiviere/Verstecke beim Darüberbewegen nach"

#. panel-indicator@ubuntuvn->settings-schema.json->hovertime->units
msgid "miliseconds"
msgstr "Millisekunden"

#. panel-indicator@ubuntuvn->settings-schema.json->hidetime->description
msgid "Autohide after "
msgstr "Verstecke automatisch nach"

#. panel-indicator@ubuntuvn->settings-
#. schema.json->hoveractivates->description
msgid "Activate on hover"
msgstr "Aktiviere beim Darüberbewegen"

#. panel-indicator@ubuntuvn->settings-
#. schema.json->hoveractivates->tooltip
msgid "show applets on mouse hover"
msgstr "Zeige Applets beim Darüberbewegen"
